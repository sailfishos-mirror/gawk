#: array.c:249
#, c-format
msgid "from %s"
msgstr "%1$s־מ"

#: array.c:355
msgid "attempt to use a scalar value as array"
msgstr "ךרעמכ ראלקסב שומיש ןויסינ"

#: array.c:357
#, c-format
msgid "attempt to use scalar parameter `%s' as an array"
msgstr "ךרעמכ `%1$s' ירלקס הנתשמב שומיש ןויסנ"

#: array.c:360
#, c-format
msgid "attempt to use scalar `%s' as an array"
msgstr "ךרעמכ `%1$s' ראלקסב שומיש ןויסינ"

#: array.c:407 array.c:574 builtin.c:88 builtin.c:1746 builtin.c:1794
#: builtin.c:1807 builtin.c:2323 builtin.c:2350 eval.c:1156 eval.c:1161 eval.c:1555
#, c-format
msgid "attempt to use array `%s' in a scalar context"
msgstr "ראלקס לש רשקהב `%1$s' ךרעמב שומיש ןויסנ"

#: array.c:581
#, c-format
msgid "delete: index `%.*s' not in array `%s'"
msgstr "delete: `%2$s' ךרעמב אצמנ אל '%1$.*s` חתפמה"

#: array.c:595
#, c-format
msgid "attempt to use scalar `%s[\"%.*s\"]' as an array"
msgstr "ךרעמכ ']"\%2$.*s"\[%1$s` ראלקסב שומיש ןויסינ"

#: array.c:808 array.c:858
#, c-format
msgid "%s: first argument is not an array"
msgstr "%1$s: ךרעמ וניא ןושארה טנמוגראה"

#: array.c:850
#, c-format
msgid "%s: second argument is not an array"
msgstr "%1$s: ךרעמ וניא ינש טנמוגרא"

#: array.c:853 field.c:1006 field.c:1100
#, c-format
msgid "%s: cannot use %s as second argument"
msgstr "%1$s: ינש טנמוגראכ `%2$s'־ב שמתשהל רוסא"

#: array.c:861
#, c-format
msgid "%s: first argument cannot be SYMTAB without a second argument"
msgstr "%1$s: SYMTAB ינש טנמוגרא אלל תויהל לוכי אל ןושאר טנמוגרא"

#: array.c:863
#, c-format
msgid "%s: first argument cannot be FUNCTAB without a second argument"
msgstr "%1$s: FUNCTAB ינש טנמוגרא אלל תויהל לוכי אל ןושאר טנמוגרא"

#: array.c:870
msgid ""
"asort/asorti: using the same array as source and destination without a third "
"argument is silly."
msgstr "asort/asorti: .ישפיט אוה ישילש טנמוגרא אלל דעיו רוקמכ ךרעמ ותואב שומיש"

#: array.c:875
#, c-format
msgid "%s: cannot use a subarray of first argument for second argument"
msgstr "%1$s: ינש טנמוגרא רובע ןושאר טנמוגרא לש הנשמ ךרעמב שמתשהל לוכי אל"

#: array.c:880
#, c-format
msgid "%s: cannot use a subarray of second argument for first argument"
msgstr "%1$s: ןושאר טנמוגרא רובע ינש טנמוגרא לש הנשמ ךרעמב שמתשהל לוכי אל"

#: array.c:1410
#, c-format
msgid "`%s' is invalid as a function name"
msgstr "`%1$s' היצקנופ םשכ יקוח וניא"

#: array.c:1414
#, c-format
msgid "sort comparison function `%s' is not defined"
msgstr "תרדגומ הניא `%1$s' ןוימה תייצקנופ"

#: awkgram.y:278
#, c-format
msgid "%s blocks must have an action part"
msgstr "הלועפ תרדגה לולכל בייח %1$s קולב"

#: awkgram.y:281
msgid "each rule must have a pattern or an action part"
msgstr "הלועפ וא תינבת וא היהיש ךירצ קוח לכל"

#: awkgram.y:435 awkgram.y:447
msgid "old awk does not support multiple `BEGIN' or `END' rules"
msgstr "awk BEGIN END וא יקוח המכב ךמות וניא ןשי"

#: awkgram.y:500
#, c-format
msgid "`%s' is a built-in function, it cannot be redefined"
msgstr "`%1$s' התרדגה תא תונשל ןיא ,תינבומ היצקנופ הניה"

#: awkgram.y:564
msgid "regexp constant `//' looks like a C++ comment, but is not"
msgstr "C++, הרעה וניא ךא תפשב הרעה ומכ הארנ `//' ירלוגרה יוטיבה"

#: awkgram.y:568
#, c-format
msgid "regexp constant `/%s/' looks like a C comment, but is not"
msgstr "C, הרעה וניא ךא תפשב הרעה ומכ הארנ `/%1$s/' ירלוגר יוטיב"

#: awkgram.y:695
#, c-format
msgid "duplicate case values in switch body: %s"
msgstr "%1$s :גתמה ףוגב תוישיר יכרע לופכ"

#: awkgram.y:716
msgid "duplicate `default' detected in switch body"
msgstr "default' גותימה ףוגב לופכ` ההוז"

#: awkgram.y:1052 awkgram.y:4484
msgid "`break' is not allowed outside a loop or switch"
msgstr "break' האלולל ץוחמ`־ב שמתשהל ןיא"

#: awkgram.y:1062 awkgram.y:4476
msgid "`continue' is not allowed outside a loop"
msgstr "continue' האלולל ץוחמ`־ב שמתשהל ןיא"

#: awkgram.y:1073
#, c-format
msgid "`next' used in %s action"
msgstr "next' %1$s לש הלועפ תרדגהב`־ב שומיש"

#: awkgram.y:1084
#, c-format
msgid "`nextfile' used in %s action"
msgstr "nextfile' %1$s לש הלועפ תרדגהב`־ב שומיש"

#: awkgram.y:1112
msgid "`return' used outside function context"
msgstr "return` היצקנופ לש טסקטנוקב אלש`-ב שומיש"

#: awkgram.y:1185
msgid "plain `print' in BEGIN or END rule should probably be `print \"\"'"
msgstr "`print' BEGIN END print \"\"'` תויהל םתסה ןמ ךירצ וא יללכב טושפ"

#: awkgram.y:1255 awkgram.y:1304
msgid "`delete' is not allowed with SYMTAB"
msgstr "`delete' SYMTAB םע רוסא"

#: awkgram.y:1257 awkgram.y:1306
msgid "`delete' is not allowed with FUNCTAB"
msgstr "`delete' FUNCTAB םע רוסא"

#: awkgram.y:1291 awkgram.y:1295
msgid "`delete(array)' is a non-portable tawk extension"
msgstr "`delete(array)' tawk ילבטרופ אל לש הבחרה וניה"

#: awkgram.y:1431
msgid "multistage two-way pipelines don't work"
msgstr "םידבוע םניא םיבלש יבורמ םיינוויכ־וד םיקיפא"

#: awkgram.y:1433
msgid "concatenation as I/O `>' redirection target is ambiguous"
msgstr "I/O `>' יעמשמ דח וניא לש שדחמ בותינ דעיכ רושרשה"

#: awkgram.y:1645
msgid "regular expression on right of assignment"
msgstr "ךרע תמשה תארוה לש ןימי דצב ירלוגר יוטיב"

#: awkgram.y:1660 awkgram.y:1673
msgid "regular expression on left of `~' or `!~' operator"
msgstr "`!~' וא `~' רוטרפואל לאמשמ ירלוגר יוטיב"

#: awkgram.y:1690 awkgram.y:1840
msgid "old awk does not support the keyword `in' except after `for'"
msgstr "awk in' for'` ירחא טעמל` יוטיבב ךמות אל ןשי"

#: awkgram.y:1700
msgid "regular expression on right of comparison"
msgstr "האוושה לש ןימי דצב ירלוגר יוטיב"

#: awkgram.y:1819
#, c-format
msgid "non-redirected `getline' invalid inside `%s' rule"
msgstr "`getline' `%1$s' ללכ ךותב יקוח אל בתונמ אל"

#: awkgram.y:1822
msgid "non-redirected `getline' undefined inside END action"
msgstr "`getline' END תלועפ ךותב רדגומ וניא היינפה אלל"

#: awkgram.y:1842
msgid "old awk does not support multidimensional arrays"
msgstr "awk םידמימ המכ לש םיכרעמב ךמות וניא ןשי"

#: awkgram.y:1945
msgid "call of `length' without parentheses is not portable"
msgstr "length' תדיינ הניא םיירגוס ילב`־ל האירק"

#: awkgram.y:2019
msgid "indirect function calls are a gawk extension"
msgstr "gawk־ל הבחרה ןניה תויצקנופל תופיקע תואירק"

#: awkgram.y:2032
#, c-format
msgid "cannot use special variable `%s' for indirect function call"
msgstr "היצקנופל הפיקע האירק ליבשב `%1$s' דחוימ הנתשמב שמתשהל רוסא"

#: awkgram.y:2065
#, c-format
msgid "attempt to use non-function `%s' in function call"
msgstr "היצקנןפ תאורקב `%1$s' היצקנופב אל אוהש רבדב שומיש ןויסנ"

#: awkgram.y:2130
msgid "invalid subscript expression"
msgstr "ךרעמ ןייצמב יוגש ריבחת לעב יוטיב"

#: awkgram.y:2505 awkgram.y:2525 gawkapi.c:274 gawkapi.c:291 msg.c:133
msgid "warning: "
msgstr " :הרהזא"

#: awkgram.y:2523 gawkapi.c:246 gawkapi.c:289 msg.c:165
msgid "fatal: "
msgstr " :עירכמ"

#: awkgram.y:2576
msgid "unexpected newline or end of string"
msgstr "יופצ-יתלב םוקמב הרוש ףוס"

#: awkgram.y:2597
msgid ""
"source files / command-line arguments must contain complete functions or rules"
msgstr ""
"םיללכ וא תואלמ תויצקנופ ליכהל םיבייח הדוקפה תרוש לש םיטנמוגרא / רוקמ יצבוק"

#: awkgram.y:2881 awkgram.y:2959 awkgram.y:3197 debug.c:545 debug.c:561
#: debug.c:2844 debug.c:5213
#, c-format
msgid "cannot open source file `%s' for reading: %s"
msgstr "%2$s :האירקל `%1$s' רוקמה ץבוק תא חותפל ןתינ אל"

#: awkgram.y:2882 awkgram.y:3019
#, c-format
msgid "cannot open shared library `%s' for reading: %s"
msgstr "%2$s :האירקל `%1$s' תפתושמה הייקיתה תא חותפל ןתינ אל"

#: awkgram.y:2884 awkgram.y:2960 awkgram.y:3020 builtin.c:136 debug.c:5364
msgid "reason unknown"
msgstr "העודי-יתלב הביסמ"

#: awkgram.y:2893 awkgram.y:2917
#, c-format
msgid "cannot include `%s' and use it as a program file"
msgstr "תינכות ץבוקכ וב שמתשהלו `%1$s' לולכל לוכי אל"

#: awkgram.y:2906
#, c-format
msgid "already included source file `%s'"
msgstr "ללכנ רבכ `%1$s' רוקמ ץבוק"

#: awkgram.y:2907
#, c-format
msgid "already loaded shared library `%s'"
msgstr "ללכנ רבכ `%1$s' תפתושמ הירפס ץבוק"

#: awkgram.y:2944
msgid "@include is a gawk extension"
msgstr "@include gawk־ל תיתדוקנ הבחרה איה"

#: awkgram.y:2950
msgid "empty filename after @include"
msgstr "include@ ירחא קיר ץבוק םש"

#: awkgram.y:2999
msgid "@load is a gawk extension"
msgstr "@load gawk-ל תיפיצפס הבחרה וניה"

#: awkgram.y:3006
msgid "empty filename after @load"
msgstr "load@ ירחא קיר ץבוק םש"

#: awkgram.y:3149
msgid "empty program text on command line"
msgstr "הדוקפה תרושמ הלבקתה הקיר תינכת"

#: awkgram.y:3265 debug.c:470 debug.c:628
#, c-format
msgid "cannot read source file `%s': %s"
msgstr "%2$s :`%1$s' רוקמה ץבוק תא אורקל ןתינ אל"

#: awkgram.y:3276
#, c-format
msgid "source file `%s' is empty"
msgstr "קיר `%1$s' רוקמה ץבוק"

#: awkgram.y:3336
#, c-format
msgid "error: invalid character '\\%03o' in source code"
msgstr "\\%1$03o' יוגש רוקמה דוקב` ותה :האיגש"

#: awkgram.y:3563
msgid "source file does not end in newline"
msgstr "השדח הרושב םייתסמ וניא רוקמה ץבוק"

#: awkgram.y:3673
msgid "unterminated regexp ends with `\\' at end of file"
msgstr "ץבוקה ףסב '\\`-ב םייתסמ רומג-יתלב ירלוגר יוטיב"

#: awkgram.y:3700
#, c-format
msgid "%s: %d: tawk regex modifier `/.../%c' doesn't work in gawk"
msgstr "%1$s: %2$d: tawk gawk-ב דבוע אל לש '%3$c/.../` ירלוגר יוטיב הנשמ"

#: awkgram.y:3704
#, c-format
msgid "tawk regex modifier `/.../%c' doesn't work in gawk"
msgstr "tawk gawk-ב דבוע אל לש '%1$c/.../` ירלוגר יוטיב הנשמ"

#: awkgram.y:3717
msgid "unterminated regexp"
msgstr "רומג-יתלב ירלוגר יוטיב"

#: awkgram.y:3721
msgid "unterminated regexp at end of file"
msgstr "ץבוקה ףוסב רומג-יתלב ירלוגר יוטיב"

#: awkgram.y:3810
msgid "use of `\\ #...' line continuation is not portable"
msgstr "יליבטרופ וניא הרוש ךשמהל ןמיסכ '...# \\`-ב שומיש"

#: awkgram.y:3832
msgid "backslash not last character on line"
msgstr "הרושב ןורחא ות וניאש ךופה ןסכול"

#: awkgram.y:3880 awkgram.y:3882
msgid "multidimensional arrays are a gawk extension"
msgstr "gawk לש הבחרה םה םיידממ-בר םיכרעמ"

#: awkgram.y:3907 awkgram.y:3918
#, c-format
msgid "POSIX does not allow operator `%s'"
msgstr "POSIX `%1$s' רוטרפוא השרמ וניא"

#: awkgram.y:3909 awkgram.y:3920 awkgram.y:3955 awkgram.y:3963
#, c-format
msgid "operator `%s' is not supported in old awk"
msgstr "awk ןשי-ב ךמתנ וניא `%1$s' רוטרפוא"

#: awkgram.y:4060 awkgram.y:4082 command.y:1188
msgid "unterminated string"
msgstr "הרומג-יתלב תזורחמ"

#: awkgram.y:4070 main.c:1284
msgid "POSIX does not allow physical newlines in string values"
msgstr "POSIX תזורחמ יכרעב תויזיפ תושדח תורוש רשפאמ וניא"

#: awkgram.y:4072 node.c:460
msgid "backslash string continuation is not portable"
msgstr "יליביטרופ וניא ירוחא יוטנ תזורחמ ךשמה"

#: awkgram.y:4313
#, c-format
msgid "invalid char '%c' in expression"
msgstr "יוטיבב '%1$c' יוגש ות"

#: awkgram.y:4408
#, c-format
msgid "`%s' is a gawk extension"
msgstr "`%1$s' gawk־ל הבחרה איה"

#: awkgram.y:4413
#, c-format
msgid "POSIX does not allow `%s'"
msgstr "POSIX `%1$s' השרמ וניא"

#: awkgram.y:4421
#, c-format
msgid "`%s' is not supported in old awk"
msgstr "`%1$s' awk ןשי-ב ךמתנ וניא"

#: awkgram.y:4521
msgid "`goto' considered harmful!"
msgstr "`goto' !ינסרה בשחנ"

#: awkgram.y:4590
#, c-format
msgid "%d is invalid as number of arguments for %s"
msgstr "%1$d %2$s רובע םיטנמוגרא לש יוגש רפסמ אוה"

#: awkgram.y:4625
#, c-format
msgid "%s: string literal as last argument of substitute has no effect"
msgstr "%1$s: העפשה ןיא ףילחת לש ןורחא טנמוגראכ תילולימ תזורחמל"

#: awkgram.y:4630
#, c-format
msgid "%s third parameter is not a changeable object"
msgstr "sub' תונשל ןתינש טירפ וניא` לש ישילש %1$s טנמוגרא"

#: awkgram.y:4734 awkgram.y:4737
msgid "match: third argument is a gawk extension"
msgstr "match' gawk-ל תיפיצפס הבחרה וניה` לש ישילש טנמוגרא"

#: awkgram.y:4791 awkgram.y:4794
msgid "close: second argument is a gawk extension"
msgstr "close' gawk-ל תיפיצפס הבחרה וניה` לש ינש טנמוגרא"

#: awkgram.y:4806
msgid "use of dcgettext(_\"...\") is incorrect: remove leading underscore"
msgstr "dcgettext(\"...\"): טנמוגרא תמדקבש ןותחת-ףקמ קלסל שי-ב יוגש שומיש"

#: awkgram.y:4821
msgid "use of dcngettext(_\"...\") is incorrect: remove leading underscore"
msgstr "dcngettext(\"...\"): טנמוגרא תמדקבש ןותחת-ףקמ קלסל שי-ב יוגש שומיש"

#: awkgram.y:4840
msgid "index: regexp constant as second argument is not allowed"
msgstr "index: רתומ וניא ינש טנמוגראכ ירלוגר יוטיב עובק"

#: awkgram.y:4893
#, c-format
msgid "function `%s': parameter `%s' shadows global variable"
msgstr "יללכ הנתשמ לע רבוג `%2$s' רטמרפה :`%1$s' היצקנופה"

#: awkgram.y:4942 debug.c:4197 debug.c:4240 debug.c:5362 profile.c:110
#, c-format
msgid "could not open `%s' for writing: %s"
msgstr ")%2$s( הלקת הביתכ ךרוצל `%1$s' תחיתפ תעב"

#: awkgram.y:4943
msgid "sending variable list to standard error"
msgstr "ינקת תואיגש ץורעל חלשנ םינתשמ תמישר"

#: awkgram.y:4951
#, c-format
msgid "%s: close failed: %s"
msgstr "%1$s: %2$s :תריגס תעב הלקת"

#: awkgram.y:4976
msgid "shadow_funcs() called twice!"
msgstr "shadow_funcs() !םיימעפ הארקנ"

#: awkgram.y:4984
msgid "there were shadowed variables"
msgstr "םיליפאמ םינתשמ ויה"

#: awkgram.y:5061
#, c-format
msgid "function name `%s' previously defined"
msgstr "`%1$s' םשב היצקנופ תרדגומ רבכ"

#: awkgram.y:5112
#, c-format
msgid "function `%s': cannot use function name as parameter name"
msgstr "רטמרפ םשכ שמשל לוכי וניא היצקנופ םש :`%1$s' היצקנופב"

#: awkgram.y:5115
#, c-format
msgid "function `%s': cannot use special variable `%s' as a function parameter"
msgstr "רטמרפ םשכ שמשל לוכי וניא דחוימ הנתשמ םש `%2$s' :`%1$s' היצקנופב"

#: awkgram.y:5119
#, c-format
msgid "function `%s': parameter `%s' cannot contain a namespace"
msgstr "םש בחרמ ליכהל לוכי אל `%2$s' רטמרפה :`%1$s' היצקנופה"

#: awkgram.y:5126
#, c-format
msgid "function `%s': parameter #%d, `%s', duplicates parameter #%d"
msgstr "%4$d ׳סמ רטמרפ תא לפכשמ ,`%3$s' ,%2$d ׳סמ רטמרפ :`%1$s' היצקנופה"

#: awkgram.y:5215
#, c-format
msgid "function `%s' called but never defined"
msgstr "תרדגומ הניאש `%1$s' היצקנופל האירק"

#: awkgram.y:5219
#, c-format
msgid "function `%s' defined but never called directly"
msgstr "תארקנ אל ךא ,הרדגוה `%1$s' היצקנופ"

#: awkgram.y:5251
#, c-format
msgid "regexp constant for parameter #%d yields boolean value"
msgstr "ינאילוב ךרע בינמ %1$d ׳סמ הנתשמל עובק ירלוגר יוטיב"

#: awkgram.y:5266
#, c-format
msgid ""
"function `%s' called with space between name and `(',\n"
"or used as a variable or an array"
msgstr ""
"n\,'(` ןיבל המש ןיב םיחוור םע `%1$s' היצקנןפל האירק"
"ךרעמ וא הנתשמכ שמשמ וא"

#: awkgram.y:5489 awkgram.y:5542 mpfr.c:1589 mpfr.c:1624
msgid "division by zero attempted"
msgstr "ספאב הקולח ןויסנ"

#: awkgram.y:5498 awkgram.y:5551 mpfr.c:1634
#, c-format
msgid "division by zero attempted in `%%'"
msgstr "`%1$%'-ב ספאב הקולח ןויסנ"

